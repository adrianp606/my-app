{
    "extends": ["react-app", "react-app/jest", "prettier/react", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error", { "singleQuote": true }],
        "import/first": 0
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint"
}
